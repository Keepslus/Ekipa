<html>
  <head>
    <title>ale fajne</title>
    <style>
      .button {
        background-color: blue;
        border-radius: 50%;
        width: 50px;
        height: 50px;
        font-size: 20px;
        color: white;
        text-align: center;
        line-height: 50px;
        transition: transform 0.1s ease-in-out;
      }
      .button:active {
        transform: scale(1.1);
      }
    </style>
    <script>
      function changeTitle() {
        document.title = Math.floor(Math.random() * 100);
      }
      function sendAlert() {
        var insults = [
          "You're a moron.",
          "You're a dunce.",
          "You're a buffoon.",
          "You're a clown."
        ];
        var insult = insults[Math.floor(Math.random() * insults.length)];
        alert(insult);
      }
    </script>
  </head>
  <body>
    <button class="button" onclick="changeTitle(); sendAlert();">Click me!</button>
  </body>
</html>
